{
  "AuthService": {
    "Url": "http://authservice:8080"
  },
  "ReverseProxy": {
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "auth-destination": {
            "Address": "http://authservice:8080"
          }
        }
      },
      "books-cluster": {
        "Destinations": {
          "books-destination": {
            "Address": "http://bookservice:8080"
          }
        }
      },
      "warehouse-cluster": {
        "Destinations": {
          "warehouse-destination": {
            "Address": "http://warehouseservice:8080"
          }
        }
      },
      "search-cluster": {
        "Destinations": {
          "search-destination": {
            "Address": "http://searchservice:8080"
          }
        }
      },
      "orders-cluster": {
        "Destinations": {
          "orders-destination": {
            "Address": "http://orderservice:8080"
          }
        }
      },
      "users-cluster": {
        "Destinations": {
          "users-destination": {
            "Address": "http://userservice:8080"
          }
        }
      },
      "notifications-cluster": {
        "Destinations": {
          "notifications-destination": {
            "Address": "http://notificationservice:8080"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
